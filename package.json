{
  "name": "Stylish-mx",
  "version": "3.0.0",
  "description": "Stylish for Maxthon",
  "scripts": {
    "dev": "gulp watch",
    "prebuild": "npm run lint && gulp clean",
    "build": "cross-env NODE_ENV=production gulp build",
    "analyze": "webpack --profile --json --config scripts/webpack.conf.js | webpack-bundle-size-analyzer",
    "analyze:json": "webpack --profile --json --config scripts/webpack.conf.js > stats.json",
    "i18n": "gulp i18n",
    "lint": "eslint --ext .js,.vue .",
    "svgo": "svgo --config .svgo.yml icons",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gera2ld/stylish-mx.git"
  },
  "author": "Gerald <gera2ld@163.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gera2ld/stylish-mx/issues"
  },
  "homepage": "https://github.com/gera2ld/stylish-mx",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.30.1",
    "js-yaml": "^3.10.0",
    "postcss-loader": "^2.0.9",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "svg-sprite-loader": "^3.5.1",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.10.0"
